{
  "name": "@bks/ui-kit",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/main.js",
  "exports": {
    ".": "./dist/main.js",
    "./components": "./dist/components/components.js",
    "./components/*": "./dist/components/*.js",
    "./utils": "./dist/utils.js",
    "./utils/*": "./dist/utils/*.js"
  },
  "types": "./dist/main.d.ts",
  "typesVersions": {
    "*": {
      "dist/main.js": [
        "dist/main.d.ts"
      ],
      "*": [
        "dist/*.d.ts",
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "lib:dev": "vite build --watch --emptyOutDir",
    "lib:types": "tsc -p ./tsconfig.json --declaration --emitDeclarationOnly --outDir dist",
    "build": "vue-tsc vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "codemirror": "^5.63.1",
    "lodash": "^4.17.21",
    "postgres-interval": "^4.0.0",
    "sql-query-identifier": "^2.8.0",
    "tabulator-tables": "beekeeper-studio/tabulator#137018661b71291b18f19324ef39484496da281b",
    "tinyduration": "^3.2.4",
    "vue": "^2.7.16",
    "vue2-teleport": "^1.1.4"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.97",
    "@types/lodash": "^4.14.159",
    "@types/tabulator-tables": "^6.2.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "typescript": "~5.6.2",
    "vite": "~5.3.6",
    "vue-tsc": "^2.1.10"
  }
}
